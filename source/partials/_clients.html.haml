%section.clients.container
  .clients__container
    - data.clients.each do |key, client|
      .clients__item
        = link_to client[:url], class: "client__link" do
          = image_tag "clients/#{key}.png", srcset: "clients/#{key}.png 1x, clients/#{key}@2x.png 2x", alt: client[:name], class: "client__image"
